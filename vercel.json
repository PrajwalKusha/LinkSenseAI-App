{
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://linksenseai-app.onrender.com/api/:match*"
    },
    {
      "source": "/:shortCode",
      "destination": "https://linksenseai-app.onrender.com/:shortCode"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
