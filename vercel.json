{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://linksenseai-app.onrender.com/api/$1"
    },
    {
      "src": "/([A-Za-z0-9_-]{6})",
      "status": 308,
      "headers": {
        "Location": "https://linksenseai-app.onrender.com/$1?redirect=true"
      },
      "has": [
        {
          "type": "query",
          "key": "redirect",
          "value": "true"
        }
      ]
    },
    {
      "src": "/([A-Za-z0-9_-]{6})",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
